{
    "version": 2,
    "builds": [
      {
        "src": "backend/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/backend/(.*)",
        "dest": "backend/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/index.js"
      }
    ]
  }
  